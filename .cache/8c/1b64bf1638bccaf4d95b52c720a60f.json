nst res = await axios({\r\n            method: 'PATCH',\r\n            url: '/api/v1/users/sendMessageAll',\r\n            data: {\r\n                title,\r\n                message\r\n            }\r\n        });\r\n        if (res.data.status === 'success') {\r\n            showAlert('Successfully sent a message to everyone.', 'success');\r\n        };\r\n    } catch (err) {\r\n        showAlert(err.response.data.message, 'fail');\r\n    }\r\n};"},"lineCount":null}},"error":null,"hash":"eed8f22a9eaffee875cd8c2fa7268499","cacheData":{"env":{}}}